{
  "name": "jk-wedding",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:css": "node-sass --importer=./sass-importer.js src/index.scss -o src",
    "start:css": "npm-run-all build:css \"build:css --watch --recursive\"",
    "build:js": "react-scripts build",
    "start:js": "react-scripts start",
    "start": "npm-run-all -p start:css start:js",
    "build": "npm-run-all build:css build:js",
    "start:prod": "yarn run build && serve -s ./build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint:sass": "sass-lint -v -q src/",
    "lint:js": "eslint .",
    "lint": "npm-run-all lint:sass lint:js"
  },
  "dependencies": {
    "@material-ui/core": "^3.0.3",
    "@material-ui/icons": "^3.0.1",
    "classnames": "^2.2.6",
    "contentful": "^7.0.3",
    "dateformat": "^3.0.3",
    "meyer-reset-scss": "^2.0.4",
    "node-sass": "^4.9.3",
    "node-sass-json-importer": "^4.0.1",
    "node-sass-tilde-importer": "^1.0.2",
    "polished": "^2.0.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-ga": "^2.5.3",
    "react-material-responsive-grid": "./lib/react-material-responsive-grid",
    "react-parallax": "^1.8.0",
    "react-scripts": "1.1.4",
    "react-scrollspy": "^3.3.5",
    "string-format": "^2.0.0",
    "styled-components": "^3.4.5",
    "vh-check": "^1.1.3"
  },
  "devDependencies": {
    "eslint": "^4",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-react": "^7.11.1",
    "npm-run-all": "^4.1.3",
    "sass-lint": "^1.12.1",
    "serve": "^10.0.1"
  },
  "description": "Jon and Kaleigh's wedding website",
  "main": "index.js",
  "repository": "git@github.com:jonscheiding/jk-wedding.git",
  "author": "Jon Scheiding <jonscheiding@gmail.com>",
  "license": "MIT"
}
