{
  "name": "jkshindig-wedding",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:4000/",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "classnames": "^2.2.6",
    "css-reset": "^0.0.1",
    "dateformat": "^3.0.3",
    "generate-password": "^1.4.0",
    "keystone": "^4.0.0",
    "meyer-reset-scss": "^2.0.4",
    "node-sass-chokidar": "^1.3.3",
    "node-sass-tilde-importer": "^1.0.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-parallax": "^1.8.0",
    "react-scripts": "1.1.4",
    "react-scrollspy": "^3.3.5",
    "styled-components": "^3.4.4"
  },
  "scripts": {
    "build:css": "node-sass-chokidar --importer=node_modules/node-sass-tilde-importer src/index.scss -o src",
    "build:js": "react-scripts build",
    "build": "npm-run-all build:css build:js",
    "start:css": "yarn run build:css --watch --recursive",
    "start:server": "babel-watch src/server/index.js",
    "start:js": "react-scripts start",
    "start": "npm-run-all -p start:css start:js start:server",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "npm-run-all lint:sass lint:js",
    "lint:sass": "sass-lint -v -q src/",
    "lint:js": "eslint ."
  },
  "devDependencies": {
    "babel-watch": "^2.0.7",
    "eslint": "^4",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-react": "^7.11.1",
    "npm-run-all": "^4.1.3",
    "sass-lint": "^1.12.1"
  }
}
