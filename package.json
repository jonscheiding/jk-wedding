{
  "name": "jkshindig-wedding",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:css": "node-sass-chokidar --importer=node_modules/node-sass-tilde-importer src/index.scss -o src",
    "start:css": "yarn run build:css --watch --recursive",
    "build:js": "react-scripts build",
    "start:js": "react-scripts start",
    "start": "npm-run-all -p start:css start:js",
    "build": "npm-run-all build:css build:js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint:sass": "sass-lint -v -q src/",
    "lint:js": "eslint .",
    "lint": "npm-run-all lint:sass lint:js"
  },
  "dependencies": {
    "node-sass-chokidar": "^1.3.3",
    "node-sass-tilde-importer": "^1.0.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.4"
  },
  "devDependencies": {
    "eslint": "^4",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-react": "^7.11.1",
    "npm-run-all": "^4.1.3",
    "sass-lint": "^1.12.1"
  }
}
